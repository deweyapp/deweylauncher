{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Dewey launcher",
  "description": "Extension to add a launch icon for Dewey",

  "background": {
    "scripts": [ "launcher.js" ]
  },
  "browser_action": {
    "default_icon": "icon_16.png"
  },
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png"
  },
  "permissions": [ "tabs", "management" ]
}